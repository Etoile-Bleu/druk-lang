// Args + Input example
ལས་ཀ་ རྩ_བ་() {
    འབྲི་ "argc:";
    འབྲི་ argc;
    འབྲི་ "argv[0]:";
    འབྲི་ argv[༠];

    འབྲི་ "Dzongkha argv count:";
    འབྲི་ ནང་འཇུག་གྲངས་;

    གལ་ཏེ་ (argc > ༡) {
        འབྲི་ "argv[1]:";
        འབྲི་ argv[༡];
    }

    འབྲི་ "Enter your name:";
    གྲངས་ཀ་ name = ནང་འཇུག་();
    འབྲི་ name;
}

རྩ_བ་();
