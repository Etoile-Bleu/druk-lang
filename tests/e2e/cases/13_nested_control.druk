ལས་འགན་ སྲུང་(གྲངས་ n) {
    གལ་སྲིད་ (n > ༠) {
        བཀོད་ n;
        ཡང་བསྐྱར་ (n > ༠) {
            གལ་སྲིད་ (n == ༥) {
                བཀོད་ "ལྔ་";
            } མེད་ན་ {
                བཀོད་ n;
            }
            n = n - ༡;
        }
    }
}

བཀོད་ "འགོ་བཙུགས་";
སྲུང་(༡༠);
བཀོད་ "མཇུག་བསྡུ་";
